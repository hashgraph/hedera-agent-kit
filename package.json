{
  "name": "hedron-websocket-agent",
  "version": "1.0.0",
  "description": "Hedera WebSocket Agent deployment for Railway",
  "main": "typescript/examples/langchain/websocket-agent.ts",
  "scripts": {
    "build": "npm run install:typescript && npm run install:agent && npm run build:shared",
    "install:typescript": "cd typescript && npm install",
    "install:agent": "cd typescript/examples/langchain && npm install",
    "build:shared": "cd typescript && npm run build:shared || npm run build || echo 'Build completed'",
    "start": "cd typescript/examples/langchain && npx ts-node websocket-agent.ts",
    "start:prod": "cd typescript/examples/langchain && npx ts-node websocket-agent.ts",
    "dev": "cd typescript/examples/langchain && npm run start:websocket",
    "postinstall": "npm run install:typescript && npm run install:agent"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@hashgraph/sdk": "^2.68.0",
    "@langchain/core": "^0.3.66",
    "@langchain/openai": "^0.6.2",
    "@types/ws": "^8.18.1",
    "dotenv": "^17.2.0",
    "ethers": "^6.13.0",
    "langchain": "^0.3.30",
    "prompts": "^2.4.2",
    "ws": "^8.18.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "keywords": ["hedera", "websocket", "agent", "blockchain"],
  "author": "Hedron Team",
  "license": "Apache-2.0"
}