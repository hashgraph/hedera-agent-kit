{
  "printWidth": 120,
  "tabWidth": 4,
  "useTabs": false,
  "singleQuote": false,
  "bracketSpacing": true,
  "plugins": [
    "prettier-plugin-solidity"
  ],
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "parser": "solidity-parse",
        "tabWidth": 4,
        "printWidth": 120,
        "singleQuote": false,
        "bracketSpacing": true
      }
    }
  ]
}